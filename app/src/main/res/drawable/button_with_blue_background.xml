<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:enterFadeDuration="100" android:exitFadeDuration="500" >
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/ios_style_blue"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/ios_style_blue_high_light"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
</selector>